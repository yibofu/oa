<div v-if="isShow" class="add-position">
    <div class="add-position-outline">
        <div class="position-title" v-if="editWhich!=''">修 改 合 同 公 司</div>
        <div class="position-title" v-else>新 增 合 同 公 司</div>
        <div style="float: left; line-height: 40px; margin-right: 5px;"><span>合同公司</span></div>
        <div class="companys-input">
            <el-input
                    class="company-input"
                    placeholder="请输入内容"
                    v-model="companyInfo.name"
                    clearable>
            </el-input>
        </div>
        <div class="btn-place">
            <el-button @click="checkBtn(0)">取 消</el-button>
            <el-button @click="checkBtn(1)" type="primary">保 存</el-button>
        </div>
    </div>
</div>