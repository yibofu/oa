.avatar-uploader .el-upload {
    border: 1px dashed #d9d9d9;
    border-radius: 6px;
    cursor: pointer;
    position: relative;
    overflow: hidden;
}
.avatar-uploader{
    display: flex;
    justify-content: center;
    align-items: center;
}
.avatar-uploader .el-upload:hover {
    border-color: #409EFF;
}
.avatar-uploader-icon {
    font-size: 28px;
    color: #8c939d;
    width: 178px;
    height: 178px;
    line-height: 178px;
    text-align: center;
}
.avatar {
    width: 178px;
    height: 178px;
    display: block;
}
.tip{
    width: 100%;
    text-align: center;
    margin-bottom: 20px;
}
.el-collapse-item__header{
    font-size: 18px;
    color: #409EFF;
}
.el-form-item{
    width: 30%;
}
.el-form-item__label{
    font-size: 16px;
    font-weight: bold;
}
.el-form-item__content{
    width:66%;
}
.el-form-item__label{
    width:130px;
}
.el-row{
    display: flex;
    justify-content: center;
    align-items: center;
}
.bottom-con{
    width:100%;
    height: 50px;
    background-color: #303133;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}
.file{
    position: absolute;
    top:0;
    left: 0;
    width:100%;
    height:100%;
    opacity: 0;
    cursor: pointer;
}