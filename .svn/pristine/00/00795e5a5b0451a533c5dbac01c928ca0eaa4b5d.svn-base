<?php
namespace View\Controller;
use Think\Controller;
class PersonnelController extends Controller {
    public function index($debug=0){
        if($debug == 0)
            $this->display("web/personnel/dist/teamwork");
        else
            $this->display("web/personnel/department/department");
    }
    public function staff($debug=0) {
        if($debug == 0)
            $this->display("web/personnel/dist/staff");
        else
            $this->display("web/personnel/staff/staff");
    }


}