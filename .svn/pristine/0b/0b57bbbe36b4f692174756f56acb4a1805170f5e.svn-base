.app-wrapper
{
    width: 100%;

    padding: 62px 0 0 180px;
    min-height: 100%;
}

.sidebar-container {
    transition: width .28s;
    width: 180px!important;
    height: 100%;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    padding-top: 62px;
    box-sizing: border-box;
    z-index: 1001;
}

.topbar-container {
    width: 100%;
    height: 61px;
    position: fixed;
    top: 0;
    left: 0;
}
.el-menu-vertical-demo,
.siderow,
.sidecol
{
    height: 100%;
}

.el-menu-item a
{
    text-decoration: none;
}

.el-submenu .el-menu-item
{
    min-width: auto!important;
}
.el-tabs--card>.el-tabs__header .el-tabs__item.is-active,.el-tabs__item.is-active{
    background-color: #409EFF;
}
.table-botton{
    width: 26px;
    height: 20px;
    padding: 0;
    /*text-align: center;*/
    /*font-size: 10px;*/
}
.table-botton i{
    width: 10px;
    height: 10px;
}