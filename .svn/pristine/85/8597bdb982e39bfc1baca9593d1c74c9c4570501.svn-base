.add-social-security-staff{
    position: fixed;
    width: 100%;
    height:100%;
    background-color: rgba(0,0,0,.5);
    top: 0;
    left: 0;
    z-index: 2000;
}
.add-social-security-outline-staff{
    margin: 0 auto;
    margin-top: 10%;
    width: 40%;
    padding: 30px;
    background-color: #fff;
    border-radius: 5px;
}
.position-title{
    width: 100%;
    text-align: center;
    font-size: 22px;
    margin-bottom: 20px;
}
.position-input{
    width: 70%;
    margin-bottom: 20px;
}
.btn-place{
    position: relative;
    width: 100%;
    display: flex;
    justify-content: center;
    margin-top: 20px;
}
.btn-place button{
    margin: 0 auto;
}
.line-staff-in{
    width: 48%;
    display: inline-block;
    margin-bottom: 15px;
}